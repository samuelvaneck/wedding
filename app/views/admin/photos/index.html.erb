<div class="d-flex justify-content-center mt-3">
  <div><h2 class="">Photos</h2></div>
</div>
<div class="d-flex mb-2">
  <div class="btn btn-outline-primary">
    <%= link_to "New", new_admin_photo_path %>
  </div>
  <div class="ml-auto">
    <input id="search" type="text" class="form-control" placeholder="Search..." autocomplete="off" data-table="photos-list" data-search-path="<%= admin_photos_path %>" />
  </div>
</div>
<div class="d-flex">
  <ul class="list-group" id="photos-list">
    <%= render @photos %>
  </ul>
</div>

<script>
  $("#search").on("keyup", function() { searchItems($(this)) });
</script>
